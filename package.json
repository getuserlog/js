{
  "name": "@userlog/js",
  "version": "1.0.0",
  "description": "userlog JS Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:deno": "denoify",
    "build:vite": "tsc && vite build",
    "dev:vite": "vite",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint --ext .ts src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getuserlog/js.git"
  },
  "keywords": [
    "userlog",
    "events",
    "logging",
    "analytics",
    "insights",
    "monitoring",
    "saas"
  ],
  "author": "Michael <michael@8byte.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getuserlog/js/issues"
  },
  "homepage": "https://github.com/getuserlog/js#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "denoify": "^1.4.1",
    "eslint": "^8.6.0",
    "prettier": "^2.5.1",
    "typescript": "^4.2.3",
    "vite": "^3.2.7"
  }
}
